{
  "name": "jsext",
  "version": "1.0.0",
  "description": "This library will extends JavaScript with JQuery Methods, You do not need to install JQuery for that. Most common methods of JQuery will be available as native methods on HTML Elements",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "rimraf dist && npm link && nodemon",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riyasat/JsExt.git"
  },
  "keywords": [
    "ExtendedJavascirpt",
    "Extended",
    "Javascript",
    "JQuery"
  ],
  "author": "Riyasat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/riyasat/JsExt/issues"
  },
  "homepage": "https://github.com/riyasat/JsExt#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "commonjs": "0.0.1",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.7",
    "ts-jest": "^26.4.4"
  },
